{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es5",
		"lib": ["dom", "es6"],
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strictNullChecks": true,
		"declaration": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"outDir": "dist"
	},
	"exclude": [
		"node_modules",
		"dist"
	]
}